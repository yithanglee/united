<div id="main-content">
	
</div>
<script type="text/javascript">

	document.addEventListener('keydown', function(event) {
      if (event.ctrlKey && event.key === '.') {
          repeatAction()
      }
  });

  function repeatAction() {
		$.ajax({url: "/html/landing.html"}).done((html) => {
			$("#main-content").html(html)
		})

  }
  repeatAction()

</script>